  <DataSets>
    <DataSet Name="GroupByStock">
      <Query>
        <DataSourceName>GroupByStock</DataSourceName>
        <CommandText>SELECT StockName AS Stocks, SUM(StockVolume) AS [Amount of Sales], CalendarYear AS Year
FROM     [ Stock Details ]
GROUP BY StockName, CalendarYear
ORDER BY Stocks, Year DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Stocks">
          <DataField>Stocks</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount_of_Sales">
          <DataField>Amount of Sales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
